<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.stepyen.kotlinbaseproject">

    <application
            android:name="com.stepyen.xframe.base.BaseApplication"
            android:allowBackup="false"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.stepyen.kotlinbaseproject.mvp.ui.activity.TestActivity">
        </activity>

        <meta-data
                android:name="com.stepyen.commonsdk.core.GlobalConfiguration"
                android:value="ConfigModule"/>

        <activity android:name="com.stepyen.kotlinbaseproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>